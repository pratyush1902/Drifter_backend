{
  "kind": "collectionType",
  "collectionName": "destinations",
  "info": {
    "singularName": "destination",
    "pluralName": "destinations",
    "displayName": "Destination",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "unique": true
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "travel_spots": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::travel-spot.travel-spot",
      "mappedBy": "destination"
    },
    "Avgtemp": {
      "type": "string"
    },
    "Rainfall": {
      "type": "string"
    },
    "Snowfall": {
      "type": "string"
    },
    "state": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::state.state",
      "inversedBy": "destinations"
    },
    "Popular": {
      "type": "boolean"
    },
    "DestinatonOfTheWeek": {
      "type": "boolean"
    },
    "Latitude": {
      "type": "decimal"
    },
    "Longitude": {
      "type": "decimal"
    },
    "food_outlets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::food-outlet.food-outlet",
      "mappedBy": "destination"
    },
    "activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activitie.activitie",
      "mappedBy": "destination"
    },
    "Season": {
      "type": "enumeration",
      "enum": [
        "Peak Season",
        "Moderate Season",
        "Off Season "
      ]
    },
    "WhyVisit": {
      "type": "json"
    },
    "WhynotVisit": {
      "type": "json"
    },
    "Traveltips": {
      "type": "json"
    },
    "MoneySave": {
      "type": "json"
    },
    "ScamSafe": {
      "type": "json"
    },
    "Contributer": {
      "type": "json"
    },
    "Destination_Gallary": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "LocalBus": {
      "type": "json"
    },
    "LocalCab": {
      "type": "json"
    },
    "LocalAuto": {
      "type": "json"
    },
    "shoopings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::shooping.shooping",
      "mappedBy": "destination"
    },
    "DestinationFood": {
      "type": "component",
      "repeatable": true,
      "component": "destination.destination-food"
    },
    "shooping_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::shooping-item.shooping-item",
      "mappedBy": "destination"
    },
    "slug": {
      "type": "uid",
      "targetField": "Name"
    },
    "Reachroad": {
      "type": "json"
    },
    "Reachtrain": {
      "type": "json"
    },
    "Reachplane": {
      "type": "json"
    },
    "Reachbus": {
      "type": "json"
    }
  }
}
