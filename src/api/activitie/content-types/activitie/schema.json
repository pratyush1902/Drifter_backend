    "category": {
      "type": "string"
    },
    "Inclsion": {
      "type": "json"
    },
    "Exclusion": {
      "type": "json"
    },
    "location": {
      "type": "text"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "activity"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "callback_requests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::callback.callback",
      "mappedBy": "activity"
    },
    "travel_spot": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::travel-spot.travel-spot",
