{
  "kind": "collectionType",
  "collectionName": "travel_spots",
  "info": {
    "singularName": "travel-spot",
    "pluralName": "travel-spots",
    "displayName": "Travel Spot",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "destination": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::destination.destination",
      "inversedBy": "travel_spots"
    },
    "Type": {
      "type": "string"
    },
    "Thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "SpotPicture": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "SpotLocation": {
      "type": "text"
    },
    "Spotbio": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "Name"
    },
    "activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activitie.activitie",
      "mappedBy": "travel_spot"
    },
    "Howtoreach": {
      "type": "json"
    },
    "Entryfees": {
      "type": "json"
    },
    "Entrytiming": {
      "type": "json"
    },
    "travelTips": {
      "type": "json"
    },
    "Travelinsights": {
      "type": "json"
    },
    "SpotBackground": {
      "type": "json"
    }
  }
}
